<p id="notice"><%= notice %></p>

<h1>Posts</h1>
<% if logged_in? %>
  <%= link_to 'Redactar post', new_post_path %>
<% end %>
<ul>
    <% @posts.each do |post| %>
      <li>
        <h4><%= link_to post.titulo, post %></h4>
        <p>Autor: <%= post.autor.username %> | Publicado el <%= post.fecha %></p>
        <td></td>
      </li>
    <% end %>
</ul>
